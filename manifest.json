{
  "author": "Mihail Chilyashev",
  "version": "0.0.1",
  "manifest_version": 2,
  "name": "urm_source changer",

  "description": "This extension replaces the utm_source part of URLs with \"fuck_your_analytics\"",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["background", "tabs", "webRequest", "webRequestBlocking", "http://*/*", "http://*/*"],

  "background": {
    "scripts": ["background.js"],
    "persistent": true
  }
}